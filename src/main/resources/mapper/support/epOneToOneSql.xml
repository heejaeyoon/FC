<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.fc.support.supportDao.EpOneToOneDao">

    <!-- 기업 1대1문의글 등록 -->
    <insert id="epPersonalInquire" parameterType="com.example.fc.support.supportVo.EpOneToOneVo">
        insert into ep_onetoone(title, detail,email,categories, create_date)
        values (#{title}, #{detail},#{email},#{categories}, now());
    </insert>




</mapper>