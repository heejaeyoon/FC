<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.fc.dao.MemberDao">
    
    <select id="doMemberList_Bak" resultType="map">
        select * from member;
    </select>
