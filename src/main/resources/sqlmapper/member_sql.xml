        <result column="name" property="name" jdbcType="NVARCHAR" javaType="String" />
        <result column="password" property="password" jdbcType="NVARCHAR" javaType="String" />
        <result column="email" property="email" jdbcType="NVARCHAR" javaType="String" />
    </resultMap>
    -->
    <select id="doMemberList" resultType="com.example.fc.vo.Vo_member">
        select * from member;
    </select>
    
    <!-- 유저 회원가입 Insert -->
    <insert id="memberJoin">
        INSERT INTO member(name,email,memberId,password)
        values (#{name}, #{email}, #{memberId}, #{password})
    </insert>

